{
  "name": "jazzypop",
  "version": "4.4.0",
  "description": "Kawaii Quiz App for Adobe Learning Manager",
  "private": true,
  "workspaces": [
    "kawaii-quiz-app",
    "kawaii-quiz-backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd kawaii-quiz-backend && npm run dev",
    "dev:frontend": "cd kawaii-quiz-app && python3 -m http.server 8080",
    "start:backend": "cd kawaii-quiz-backend && npm start",
    "test": "npm run test:backend",
    "test:backend": "cd kawaii-quiz-backend && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/JazzyPop.git"
  },
  "keywords": [
    "quiz",
    "adobe-learning-manager",
    "alm",
    "kawaii",
    "education",
    "native-extension"
  ],
  "contributors": [
    {
      "name": "p0qp0q",
      "role": "Project Lead & Frontend Development"
    },
    {
      "name": "Claude (Anthropic)",
      "role": "Architecture & Implementation"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/JazzyPop/issues"
  },
  "homepage": "https://github.com/yourusername/JazzyPop#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}